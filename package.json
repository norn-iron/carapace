{
  "name": "@norn-iron/carapace",
  "version": "0.0.1",
  "description": "React Native component library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "pnpm build",
    "dev": "pnpm build && pnpm build:watch & pnpm --filter carapace-docs start",
    "dev:ios": "pnpm dev --ios",
    "dev:android": "pnpm dev --android",
    "dev:web": "pnpm dev --web",
    "format": "biome format . --write && biome check --write",
    "format:check": "biome format . && biome check --no-errors-on-unmatched",
    "lint": "biome lint . --write",
    "lint:check": "biome lint . --no-errors-on-unmatched",
    "typecheck": "tsc",
    "check-version": "bash scripts/check-version-bump.sh",
    "sweep": "pnpm -r format && pnpm -r lint && pnpm -r typecheck",
    "sweep:check": "pnpm -r format:check && pnpm -r lint:check && pnpm -r typecheck"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@biomejs/biome": "^2.3.8",
    "@types/react": "~19.1.0",
    "expo": "~54.0.27",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "~0.21.0",
    "typescript": "~5.9.2"
  },
  "keywords": [
    "react-native",
    "components",
    "ui",
    "library",
    "carapace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/norn-iron/carapace.git"
  },
  "author": "Colin Young",
  "license": "UNLICENSED"
}
